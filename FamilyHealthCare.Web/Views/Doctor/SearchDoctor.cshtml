                                    <a class="apt-btn" asp-area="" asp-controller="Doctor" asp-action="BookingDoctor">Book Appointment</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>                                                   
                <!-- /Doctor Widget -->
                <!-- Doctor Widget -->
